  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/create-landing-page.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/create-landing-page/issues"
  },
  "homepage": "https://github.com/yourusername/create-landing-page#readme",
